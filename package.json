{
  "name": "big-o-for-vscode",
  "displayName": "big O for VSCode",
  "description": "A VSCode extension that analyzes functions in JavaScript/TypeScript files and inserts a comment above each one estimating its time complexity in Big O notation.",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.99.0"
  },
  "publisher": "caldeirao",
  "author": {
    "name": "Luiz Caldeir√£o",
    "email": "claudiocaldeirao@gmail.com"
  },
  "icon": "images/logo.png",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/claudiocaldeirao/big-o-for-vscode"
  },
  "keywords": [
    "complexity",
    "big o",
    "big o notation",
    "time complexity",
    "code analysis",
    "function analysis",
    "performance",
    "vscode extension",
    "javascript",
    "typescript",
    "static analysis",
    "developer tools",
    "code metrics",
    "code quality",
    "complexity analyzer"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "big-o-for-vscode.analyzeComplexity",
        "title": "Analyze Code Complexity"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "yarn run check-types && yarn run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "yarn run check-types && yarn run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "vsce:package": "vsce package",
    "vsce:publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "esbuild": "^0.25.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
